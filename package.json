{
  "name": "asyncfilteriterator",
  "version": "0.0.1",
  "description": "An AsyncIterator extensions that allows for filtering with Promise based filters.",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "watch": "gulp watch",
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wschella/AsyncFilterIterator.git"
  },
  "keywords": [
    "async",
    "iterator",
    "promise"
  ],
  "author": "Wout Schellaert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wschella/AsyncFilterIteratorissues"
  },
  "homepage": "https://github.com/wschella/AsyncFilterIterator#readme",
  "devDependencies": {
    "@types/asynciterator": "^1.1.1",
    "@types/jest": "^21.1.4",
    "jest": "^22.4.0",
    "jest-html-reporter": "^0.5.3",
    "ts-jest": "^21.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "asynciterator": "^1.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/archive/",
      "/dist/",
      "/test/util/"
    ],
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  }
}